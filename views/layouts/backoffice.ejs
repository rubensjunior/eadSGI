<!doctype html>
<html lang="pt-br">

<head>
  <base href="/">
  <script src="/dmxAppConnect/dmxAppConnect.js"></script>
  <meta charset="UTF-8">
  <title>Spacefeed - Backoffice</title>

  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.15.4/css/all.css" integrity="sha384-DyZ88mC6Up2uqS4h/KRgHuoeGwBcD4Ng9SiP4dIRy0EXTlnuz47vAwmeGwVChigm" crossorigin="anonymous" />
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <link rel="stylesheet" href="/bootstrap/5/css/bootstrap.min.css" />
  <link rel="stylesheet" href="/css/style.css" />
  <script src="/dmxAppConnect/dmxRouting/dmxRouting.js" defer></script>
  <script src="/dmxAppConnect/dmxBrowser/dmxBrowser.js" defer></script>
</head>

<body is="dmx-app" id="backoffice">
  <div is="dmx-browser" id="navegacao"></div>
  <dmx-serverconnect id="logout" url="/api/autenticacao/logout" noload="true"></dmx-serverconnect>
  <div class="container-fluid vh-100">
    <div class="row h-100 p-0">
      <div class="col-12 col-md-3 col-xl-2 bg-dark p-3">
        <img class="img-fluid mb-5" src="/assets/images/spacefeed-white.png" width="160">
        <div class="d-flex align-items-center py-2 mb-2 mouse-hover" dmx-on:click="navegacao.goto('/b/')">
          <img src="/assets/duotune/abstract/abs029.svg" class="img-fluid me-3" width="20">
          <h6 class="text-white p-0 m-0">Dashboard</h6>

        </div>
        <div class="d-flex align-items-center py-2 mb-2 mouse-hover" dmx-on:click="navegacao.goto('/b/colaboradores')">
          <img src="/assets/duotune/abstract/abs022.svg" class="img-fluid me-3" width="20">
          <h6 class="text-white p-0 m-0">Colaboradores</h6>

        </div>
        <div class="d-flex align-items-center py-2 mb-2 mouse-hover">
          <img src="/assets/duotune/abstract/abs042.svg" class="img-fluid me-3" width="20">
          <h6 class="text-white p-0 m-0">Projetos e tarefas</h6>

        </div>
        <div class="d-flex align-items-center py-2 mb-2 mouse-hover">
          <img src="/assets/duotune/abstract/abs023.svg" class="img-fluid me-3" width="20">
          <h6 class="text-white p-0 m-0">Chamados</h6>

        </div>
      </div>
      <div class="col-12 col-md-9 col-xl-10 bg-grey">
        <div class="row p-0">
          <div class="col bg-white p-3 text-end">
            <button id="btn1" class="btn btn-primary btn-sm">Button</button>
          </div>
        </div>
        <div class="row p-0">
          <div class="col p-0"><%- await include(content, locals); %></div>
        </div>
      </div>
    </div>
  </div>
  <script src="/bootstrap/5/js/bootstrap.bundle.min.js"></script>
</body>

</html>